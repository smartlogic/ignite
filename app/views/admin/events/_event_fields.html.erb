<p>
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :date %><br />
  <%= f.datetime_select :date %>
</p>
<p>
  <%= f.check_box :is_featured %>
  <%= f.label :is_featured, "Feature this event" %>
</p>
<p>
  <%= f.check_box :is_complete %>
  <%= f.label :is_complete, "Mark this event as completed (will show up in \"Past Events\" section)" %>
</p>
<p>
  <%= f.check_box :accepting_proposals %>
  <%= f.label :accepting_proposals, "Is this event currently accepting proposals?" %>
</p>
<p>
  Organizers<br />
  <%= habtm_checkboxes(event, :organizer_ids, @organizers, :name) %>
</p>
<p>
  <%= f.label :location_name %><br />
  <%= f.text_field :location_name %>
</p>
<p>
  <%= f.label :address_line1, "Street Address" %><br />
  <%= f.text_field :address_line1 %>
</p>
<p>
  <%= f.label :address_line2, "Apt/Ste #" %><br />
  <%= f.text_field :address_line2 %>
</p>
<p>
  <%= f.label :city %><br />
  <%= f.text_field :city %>
</p>
<p>
  <%= f.label :state %><br />
  <%= state_select :event, :state %>
</p>
<p>
  <%= f.label :zip %><br />
  <%= f.text_field :zip %>
</p>
<p>
  <%= f.label :description %><br />
  <%= f.text_area :description, :style => "width:600px; height:200px;" %>
</p>
<p>
  <%= f.label :sponsors_url, "Sponsors URL" %><br />
  <%= f.text_field :sponsors_url %>
