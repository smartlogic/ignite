<p>
  <%= f.label :videos_url, "Videos URL" %><br />
  <%= f.text_field :videos_url %>
</p>
<p>
  <%= f.label :images_url, "Images URL" %><br />
  <%= f.text_field :images_url %>
</p>
<p>
  <%= f.label :rsvp_url, "RSVP URL" %><br />
  <%= f.text_field :rsvp_url %>
</p>
<p>
  <%= f.label :map_url, "Map and Directions URL" %><br />
  <%= f.text_field :map_url %>
</p>
<p>
  <%= f.label :summary_image %><br/>
  <%= file_column_field "event", :summary_image %>
</p>
<p>
  <%= f.label :summary_image_caption %><br/>
  <%= f.text_field :summary_image_caption %>
</p>