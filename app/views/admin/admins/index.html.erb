<h1>Listing admins</h1>

<table class="adminTable">
  <tr>
    <th>Name</th>
    <th>Login</th>
    <th>Email</th>
  </tr>

<% for admin in @admins %>
  <tr>
    <td><%=h admin.name %></td>
    <td><%=h admin.login %></td>
    <td><%=h admin.email %></td>
    <td><%= link_to 'Show', admin_admin_path(admin) %></td>
    <td><%= link_to 'Edit', edit_admin_admin_path(admin) %></td>
    <td><%= link_to 'Destroy', admin_admin_path(admin), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Admin', new_admin_admin_path %>
