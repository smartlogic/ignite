<h1>Edit <%= h(@ignite.name) %> Settings</h1>

<% form_for @ignite, :url => admin_ignite_path, :html => {:multipart => true}, :method => :put do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', admin_ignite_path(@ignite) %> |
<%= link_to 'Back', admin_ignites_path %>
